{
  "Logging": {
    "LogLevel": {
      "Default": "Information",
      "Microsoft.AspNetCore": "Warning",
      "Grpc": "Information",
      "AppTrace": "Debug"
    }
  },
  "AllowedHosts": "*",
  "ConnectionStrings": {
    "PostgreSQL": "Host=localhost;Database=apptrace;Username=postgres;Password=your_password"
  },
  "AppTrace": {
    "StorageType": "bulk",
    "Performance": {
      "BatchSize": 1000,
      "ConnectionPoolSize": 10,
      "CommandTimeout": 30
    }
  }
}
